{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]

        "builds": [
        { "src": "api/index.php", "use": "vercel-php@0.6.0" },
        { "src": "package.json", "use": "@vercel/static-build" }
    ]
}